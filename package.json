{
  "name": "is-url-online",
  "version": "1.5.0",
  "description": "A small library that checks whether a URL is online or not",
  "keywords": [
    "async",
    "check",
    "exists",
    "http",
    "https",
    "humanized",
    "prepend",
    "promise",
    "protocol",
    "scheme",
    "uri",
    "url",
    "validation"
  ],
  "homepage": "https://github.com/spa5k/is-url-online#readme",
  "bugs": {
    "url": "https://github.com/spa5k/is-url-online/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spa5k/is-url-online.git"
  },
  "license": "MIT",
  "author": "spa5k <unbuttun@gmail.com>",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "cz": "cz",
    "dev": "nodemon ./dist/index.js",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "semantic-release": "semantic-release",
    "pretest": "tsup",
    "test": "vitest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format && pnpm lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.5",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/node": "18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.14.54",
    "eslint": "8.21.0",
    "eslint-config-galex": "4.2.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-inclusive-language": "^2.2.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "2.7.1",
    "semantic-release": "19.0.3",
    "tslib": "2.4.0",
    "tsup": "6.2.1",
    "typescript": "4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0"
  },
  "packageManager": "pnpm@7.9.0"
}
