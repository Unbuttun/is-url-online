{
  "name": "is-url-online",
  "description": "A small library that checks whether a URL is online or not",
  "version": "1.3.3",
  "author": "spa5k <unbuttun@gmail.com>",
  "bugs": {
    "url": "https://github.com/spa5k/is-url-online/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "ohmyfetch": "^0.4.14"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/node": "17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.14.10",
    "eslint": "8.6.0",
    "eslint-config-galex": "3.5.4",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-inclusive-language": "^2.2.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "husky": "7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "semantic-release": "18.0.1",
    "tslib": "2.3.1",
    "tsup": "5.11.10",
    "typescript": "4.5.4",
    "vite": "^2.7.10",
    "vitest": "^0.0.125"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/spa5k/is-url-online#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format && pnpm lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "async",
    "check",
    "exists",
    "http",
    "https",
    "humanized",
    "prepend",
    "promise",
    "protocol",
    "scheme",
    "uri",
    "url",
    "validation"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "packageManager": "pnpm@6.24.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spa5k/is-url-online.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "nodemon ./dist/index.js",
    "cz": "cz",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "semantic-release": "semantic-release",
    "pretest": "tsup",
    "test": "vitest"
  },
  "sideEffects": false,
  "typings": "dist/index.d.ts"
}
